<div class="col-md-offset-5 col-md-8">
  <h2><%= @category.title %></h2>
</div>

<div class="photo_buttons">
  <a href="/categories/<%= @category.id %>/photos/uploader" class="btn btn-default" type="submit">Add Photo</a>
  <a href="/categories" class="btn btn-default" type="submit">Back</a>
</div>

 <div class="row">
  <div class="span16">
    <ul class="media-grid">
      <% @photos.each do |photo| %>
        <a href="/photos/<%= photo.id %>">
        <li><img src="<%= photo.photo_url %>" class="img-thumbnail" alt="Thumbnail Image"></li>
        </a>
    <% end %>
   </div>
  </div>
 </div>
</div>





